page {
  height: 100%;
  padding: 0rpx;
  background-color: #f7f7f7;
}

.page__head {
  z-index: 100;
  position: fixed;
  top: 0;
  width: 750rpx;
}

.page__body {
  z-index: -1;
  margin-top: 100rpx;
}

.headtext {
  font-weight: 700;
  font-size: 35rpx;
}

.divLine {
  background: #e0e3da;
  width: 100%;
  height: 2rpx;
}

.iconstatus {
  position: absolute;
  right: 40rpx;
  top: 20rpx;
}

.plus {
  position: absolute;
  color: rgb(42, 211, 0);
  font-weight: 700;
  font-size: 40rpx;
  right: 40rpx;
  bottom: 20rpx;
}

.text0 {
  font-weight: 500;
  font-size: 26rpx;
}

.history-slidecell {
  margin: 8rpx;
  background-color: #fff;
  border-radius: 8rpx;
  padding: 24rpx;
  line-height: 1.4;
  font-size: 17rpx;
  color: rgba(0, 0, 0, 0.9);
}
