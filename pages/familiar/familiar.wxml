<mp-searchbar bindselectresult="selectResult" search="{{search}}"></mp-searchbar>
<!-- <view>{{test_text}}</view> -->

<view class="initial" wx:for="{{initials}}" wx:key="*this" wx:for-index="initialIndex" wx:for-item="initialValue">
  <text>{{initialValue}}</text>
  <view id="wordscontainer">
    <view class="item" data-position="{{initialIndex}}.{{index}}" wx:for="{{words[initialIndex]}}" wx:key="*this" bindtap="showDetail">{{item}}</view>
  </view>
</view>

<mp-dialog title="{{dialogTitle}}" show="{{dialogShow}}" bindbuttontap="tapDialogButton" buttons="{{buttons}}">
  <view>{{dialogContent}}</view>
</mp-dialog>