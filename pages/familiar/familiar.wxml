<mp-searchbar bindselectresult="selectResult" search="{{search}}"></mp-searchbar>
<block wx:for="{{words}}" wx:key="*this">
  <i-grid class="wordgrid">
      <i-grid-item>{{item.first}}</i-grid-item>
      <i-grid-item>{{item.second}}</i-grid-item>
      <i-grid-item>{{item.third}}</i-grid-item>
  </i-grid>
</block>
